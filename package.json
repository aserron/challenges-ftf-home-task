{
  "name": "ftgh",
  "version": "0.6.0",
  "private": true,
  "workspaces": [    
    "apps/*",
    "packages/*",
    "packages/eslint-config-shared"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",   
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
  
}
